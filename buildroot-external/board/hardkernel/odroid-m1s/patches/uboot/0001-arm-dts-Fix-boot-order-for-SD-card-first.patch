+++ b/arch/arm/dts/rk3568-generic-u-boot.dtsi
@@ -5,6 +5,7 @@
 / {
 	chosen {
 		stdout-path = &uart2;
+		u-boot,spl-boot-order = &sdmmc0, &sdhci;
 	};
 };
 
